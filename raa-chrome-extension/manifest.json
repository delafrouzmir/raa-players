{
  "manifest_version": 2,

  "name": "رادیو اتو-اسعد",
  "description": "Smart playback of Radio Auto-asaad",
  "version": "1.0.4",

  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js", "jquery-3.2.1.min.js"]
  },
  "permissions": [
    "activeTab",
    "http://raa.media:8000/"
  ]
}
